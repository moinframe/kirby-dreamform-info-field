{
	"name": "moinframe/kirby-dreamform-info-field",
	"description": "Simple Info field for Kirby Dreamform",
	"license": "MIT",
	"type": "kirby-plugin",
	"keywords": [
		"kirby-plugin",
		"dreamform"
	],
	"version": "0.1.0",
	"authors": [
		{
			"name": "Justus Kraft",
			"email": "hallo@justuskraft.de"
		}
	],
	"require": {
		"getkirby/cms": "^4.0 || ^5.0",
		"getkirby/composer-installer": "^1.1"
	},
	"scripts": {
		"fix": "php-cs-fixer fix --config .php-cs-fixer.dist.php"
	},
	"config": {
		"allow-plugins": {
			"getkirby/composer-installer": true
		}
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.75"
	}
}
